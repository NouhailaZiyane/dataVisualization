@model IEnumerable<googleCharts.Models.tablette>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.deviceid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.modele)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.profilid)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.deviceid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.modele)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.profilid)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.tabid }) |
            @Html.ActionLink("Details", "Details", new { id=item.tabid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.tabid })
        </td>
    </tr>
}

</table>
